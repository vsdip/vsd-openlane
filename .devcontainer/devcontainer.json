{
  "name": "OpenLane v0.21 (built)",
  "build": { "dockerfile": "Dockerfile" },
  "remoteUser": "root",
  "overrideCommand": false,
  "init": true,
  "containerEnv": {
    "DISPLAY": ":1",
    "HOME": "/root"
  },
  "postCreateCommand": "bash -lc 'ln -sf \"$PWD\" /openLANE_flow || true; echo OPENLANE_FLOW_LINK_OK'"
}
